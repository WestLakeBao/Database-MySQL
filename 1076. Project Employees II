1076. Project Employees II

-- report all the projects that have the most employees.

-- Project table:
-- +-------------+-------------+
-- | project_id  | employee_id |
-- +-------------+-------------+
-- | 1           | 1           |
-- | 1           | 2           |
-- | 1           | 3           |
-- | 2           | 1           |
-- | 2           | 4           |
-- +-------------+-------------+

-- Result table:
-- +-------------+
-- | project_id  |
-- +-------------+
-- | 1           |
-- +-------------+

select a.project_id from 
(
    select project_id, 
    rank() over(order by count(employee_id) desc) as rk
    from 
    Project
    group by project_id
) a where a.rk=1